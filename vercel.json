{
  "buildCommand": "cd apps/frontend && npm run build",
  "outputDirectory": "apps/frontend/.next",
  "installCommand": "cd apps/frontend && npm install",
  "framework": "nextjs",
  "git": {
    "deploymentEnabled": false
  },
  "env": {
    "NEXT_PUBLIC_BACKEND_URL": "https://crm-backend-api.fly.dev",
    "NEXT_PUBLIC_WS_URL": "wss://crm-backend-api.fly.dev",
    "NEXT_PUBLIC_DEMO_MODE": "false",
    "NEXT_PUBLIC_AI_EMPLOYEES_ENABLED": "true",
    "NEXT_PUBLIC_ENVIRONMENT": "production"
  }
}