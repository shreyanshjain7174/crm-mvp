# Frontend Environment Configuration

# Application Settings
NEXT_PUBLIC_BACKEND_URL=http://localhost:3001
NEXT_PUBLIC_WS_URL=ws://localhost:3001
NEXT_PUBLIC_FRONTEND_URL=http://localhost:3000

# Feature Flags
NEXT_PUBLIC_DEMO_MODE=true
NEXT_PUBLIC_AI_EMPLOYEES_ENABLED=true
NEXT_PUBLIC_ENVIRONMENT=development

# Development Tools
NEXT_PUBLIC_ENABLE_QUERY_DEVTOOLS=true
NEXT_PUBLIC_ENABLE_CACHE_MONITOR=true

# Analytics (Optional)
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=your-ga-id
NEXT_PUBLIC_MIXPANEL_TOKEN=your-mixpanel-token

# Error Tracking (Optional)  
NEXT_PUBLIC_SENTRY_DSN=your-sentry-dsn

# Feature Gates
NEXT_PUBLIC_ADVANCED_FEATURES=true
NEXT_PUBLIC_BETA_FEATURES=false

# Langflow Integration
NEXT_PUBLIC_LANGFLOW_URL=http://localhost:7860